<manifest package="com.dht.demo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>

    <application
        android:name="com.dht.demo.GApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.dht.demo.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.dht.demo.okgo.MethodActivity"/>
        <activity android:name="com.dht.demo.okgo.JsonActivity"/>
        <activity android:name="com.dht.demo.okgo.BitmapRequestActivity"/>
        <activity android:name="com.dht.demo.okgo.UpActivity"/>
        <activity android:name="com.dht.demo.okgo.CookieActivity"/>
        <activity android:name="com.dht.demo.okgo.FormUploadActivity"/>
        <activity android:name="com.dht.demo.okgo.SimpleDownloadActivity"/>
        <activity android:name="com.dht.demo.okgo.CacheActivity"/>
        <activity android:name="com.dht.demo.okgo.HttpsActivity"/>
        <activity android:name="com.dht.demo.okgo.SyncActivity"/>
        <activity android:name="com.dht.demo.okgo.RedirectActivity"/>
        <activity android:name="com.dht.demo.okgo.TestActivity"/>
        <activity
            android:name="com.dht.demo.supercache.SuperCacheActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name="com.dht.demo.WebActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name="com.dht.demo.okgo.CommonActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name="com.dht.demo.okdownload.DownloadAllActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name="com.dht.demo.okdownload.DownloadingActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name="com.dht.demo.okdownload.DownloadFinishActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name="com.dht.demo.okdownload.DesActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name="com.dht.demo.okdownload.DownloadListActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name="com.dht.demo.okupload.UploadListActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name="com.dht.demo.okupload.UploadAllActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name="com.dht.demo.okupload.UploadFinishActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name="com.dht.demo.okupload.UploadingActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <!-- Rx相关 -->
        <activity android:name="com.dht.demo.okrx2.RxCommonActivity"/>
        <activity android:name="com.dht.demo.okrx2.RxCacheActivity"/>
        <activity android:name="com.dht.demo.okrx2.RxRetrofitActivity"/>
        <activity android:name="com.dht.demo.okrx2.RxBitmapActivity"/>
        <activity android:name="com.dht.demo.okrx2.RxFileDownloadActivity"/>
        <activity android:name="com.dht.demo.okrx2.RxFormUploadActivity"/>
    </application>

</manifest>
